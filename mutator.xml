<?xml version="1.0" encoding="UTF-8"?>
<Settings>
	<Mutation>
		<!-- Mutation home directory -->
		<Directory name="mutation_home">E:\mutatorHome</Directory>
		<!-- mutating files package directory -->
		<Directory name="package_path">E:\AndroidStudioProjects\MyApplication\app\src\main\java</Directory>
		<!-- mutating files classes directory -->
		<Directory name="classes_path">E:\AndroidStudioProjects\MyApplication\app\build\intermediates\classes\debug</Directory>
		<Directory name="mutants_path">E:\mutatorHome\result\MyApplication</Directory>
		<Directory name="AndroidManifest_path">E:\AndroidStudioProjects\MyApplication\app\src\main</Directory>
	</Mutation>
	<Builder>
		<!-- Application project name -->
		<Directory name="project_name">MyApplication</Directory>
		<!-- Application project directory -->
		<Directory name="project_path">E:\AndroidStudioProjects</Directory>
		<!-- Application project copy path. Source code is not affected when the build runs incorrectly or is interrupted-->
		<Directory name="build_path">E:\mutatorHome\pro_source</Directory>
		<!-- Mutant APKs output directory -->
		<Directory name="apk_output">E:\mutatorHome\apk_output</Directory>
	</Builder>
	<!-- python instruction path -->
	<Python>D:\Python27\python</Python>
</Settings>


